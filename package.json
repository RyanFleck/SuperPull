{
  "name": "superpull",
  "version": "1.1.0",
  "description": "Tool for pulling all local repositories, as specified in ~/.superpull",
  "main": "superpull.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint --fix superpull.js",
    "build":"pkg . --target host --output bin/superpull --debug",
    "travis-build":"pkg . --target host --output bin/superpull"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RyanFleck/SuperPull.git"
  },
  "keywords": [
    "Git",
    "VCS",
    "CLI"
  ],
  "author": "Ryan Fleck",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/RyanFleck/SuperPull/issues"
  },
  "homepage": "https://ryanfleck.github.io/SuperPull/",
  "dependencies": {
    "chalk": "^2.4.2",
    "child_process": "^1.0.2",
    "commander": "^2.19.0",
    "fs": "0.0.1-security"
  },
  "devDependencies": {
    "eslint": "^5.12.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.15.0",
    "pkg": "^4.3.7"
  },
  "bin": "superpull.js",
  "pkg": {
    "assets": "node_modules/**/*"
  }
}
